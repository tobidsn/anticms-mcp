          "attribute": {
            "caption": "Enable or disable the hero section",
            "defaultValue": true,
            "value": null
          }
        },
        {
          "name": "background_image",
          "label": "Background Image",
          "field": "media",
          "attribute": {
            "accept": [
              "image"
            ],
            "resolution": {
              "minWidth": 1200,
              "maxWidth": 1920,
              "minHeight": 600,
              "maxHeight": 1080
            },
            "caption": "Recommended resolution: 1920x1080px"
          }
        },
        {
          "name": "scroll_indicator",
          "label": "Scroll Indicator",
          "field": "toggle",
          "attribute": {
            "caption": "Show/hide scroll indicator",
            "defaultValue": true,
            "value": null
